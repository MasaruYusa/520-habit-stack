{
  "name": "520-habit-stack",
  "version": "0.1.0",
  "private": true,
  "description": "LLM-powered habit formation app for consistent 5:20 AM wake-up",
  "keywords": [
    "habit",
    "morning-routine",
    "llm",
    "nextjs"
  ],
  "author": "",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "typecheck": "turbo run typecheck",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@prisma/client": "^5.9.0",
    "@types/node": "^20.10.0",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "prisma": "^5.9.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
